{
  "name": "tankd-planner",
  "description": "A project to plan and deploy tankd applications using Astro, Cloudflare, Tailwind, and React. Also includes an archive utility for AI pair programming.",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "archive": "./archive.js"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "deploy": "wrangler pages deploy --project-name tankd-planner dist"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.2.3",
    "@astrojs/node": "^9.1.2",
    "@astrojs/react": "^4.2.1",
    "@astrojs/tailwind": "^6.0.0",
    "astro": "^5.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "wrangler": "^3.111.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tankd-planner.git"
  },
  "keywords": [
    "astro",
    "cloudflare",
    "tailwind",
    "react",
    "tankd-planner"
  ],
