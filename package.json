{
  "name": "tankd-planner",
  "description": "A project to plan and deploy tankd applications using Astro, Cloudflare, Tailwind, and React. Also includes an archive utility for AI pair programming.",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "archive": "./archive.js"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "deploy": "wrangler pages deploy --project-name tankd-planner dist",
    "debug": "wrangler pages deployment tail",
    "validate:fish": "node scripts/validate-fish.js"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.2.4",
